class AnchorInfoBinder : ItemViewBinder<VideoAnchorInfo, AnchorInfoVH>() {
    override fun onCreateViewHolder(inflater: LayoutInflater, parent: ViewGroup): AnchorInfoVH {
        val view = inflater.inflate(R.layout.item_anchor_info, parent, false)
        return AnchorInfoVH(view)
    }

    override fun onBindViewHolder(vh: AnchorInfoVH, data: VideoAnchorInfo) {
        vh.onBindData(data)
    }
}